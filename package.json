  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-modules-umd": "7.14.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jquery": "3.5.6",
